<!doctype html>
<html>
  <head>
    <!-- <link rel="stylesheet" type="text/css" href="/css/main.css"> -->
    <link rel="stylesheet" type="text/css" href="/css/styles.css">
    <script language="javascript" src="/js/jquery-1.4.1.min.js"> </script>
    <script language="javascript" src="/js/app.js"> </script>
    <link rel="stylesheet" type="text/css" href="http://drnicwilliams.com/external/CodeHighlighter/styles.css">
    
    <link rel="stylesheet" type="text/css" href="/css/code_highlighting.css">
    <script src="http://drnicwilliams.com/external/CodeHighlighter/clean_tumblr_pre.js"></script>
    <link rel="alternate" type="application/atom+xml" title="<%= title %> - feed" href="/feed.xml" />
    <title><%= title %></title>
  </head>
  <body>
    <section id="wrapper">
      <header id="header">
        <h1><a href="/"><%= title %></a></h1>
      </header>
      
      <section id="center">
        <section class="" style="width: 160px;margin: 0px auto;">
          <img src="/images/phil_pic_sq.jpg" width="160" />
          <p style="margin: 20px auto;">
            <a href="http://feeds.feedburner.com/philcrissman"><img src="http://feeds.feedburner.com/~fc/philcrissman?bg=0099ee&amp;fg=000000&amp;anim=0" height="26" width="88" style="border:0" alt="" /></a>
          </p>
        </section>
        <p></p>
        <ul>
          <li><a href="/">Home</a></li>
          <li><a href="/about">About</a></li>
          <li><a href="/archives">Archives</a></li>
        </ul>
        <section style="width: 160px;margin: 0px auto; clear:both;">
          <script type="text/javascript"><!--
          google_ad_client = "pub-6000752616661499";
          /* 125x125, created 6/1/10 */
          google_ad_slot = "3045793209";
          google_ad_width = 125;
          google_ad_height = 125;
          //-->
          </script>
          <script type="text/javascript"
          src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
          </script>
        </section>
      </section>
      
      <section id="content">
        <section class="bg left">
          <%= yield %>
        </section>
      </section>
      
      <section id="sidebar">
        <section class="bg right">  
          <h2>A heading</h2>
          <ul>
            <li>Whatever</li>
            <li>And more...</li>
          </ul>
          
          <p style="clear:both">&nbsp;</p>
        </section>
      </section>
      <footer id="footer">
        <section class="bg">
          powered by <a href="http://cloudhead.io/toto">toto</a> and <a href="">henry</a>
        </section>
      </footer>
    </section>
    <script type="text/javascript">
    var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
    document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
    </script>
    <script type="text/javascript">
    var pageTracker = _gat._getTracker("UA-77047-1");
    pageTracker._initData();
    pageTracker._trackPageview();
    </script>
  </body>
</html>

